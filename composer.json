{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "kiwilan/steward-laravel": "dev-main",
        "kiwilan/typescriptable-laravel": "^1.1",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^3.0",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.0",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8",
        "meilisearch/meilisearch-php": "^1.0",
        "opcodesio/log-viewer": "^2.3",
        "spatie/laravel-ray": "^1.32",
        "spatie/laravel-route-attributes": "^1.18",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "fruitcake/laravel-telescope-toolbar": "^1.3",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^2.4",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan optimize:clear",
            "./vendor/bin/pint --config vendor/kiwilan/steward-laravel/pint.json",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite --reset",
            "php artisan ide-helper:meta",
            "@php artisan ide-helper:eloquent"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "helper": [
            "./vendor/bin/pint --config vendor/kiwilan/steward-laravel/pint.json",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite --reset",
            "php artisan ide-helper:meta",
            "@php artisan ide-helper:eloquent"
        ],
        "format": [
            "./vendor/bin/pint --config vendor/kiwilan/steward-laravel/pint.json"
        ],
        "analyse": [
            "phpstan analyse --ansi --memory-limit=4G"
        ],
        "insights": [
            "php artisan insights --ansi --no-interaction"
        ],
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan serve"
        ],
        "test": [
            "@php artisan test"
        ],
        "test:filter": [
            "@php artisan test --filter"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch"
        ],
        "test:filter:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch --filter"
        ],
        "queue:listen": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan queue:listen --tries=3 --timeout=3600"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
